set group_subprocesses Auto
set ignore_six_quark_processes False
set loop_optimized_output True
set complex_mass_scheme False
import model HEL_UFO-no_b_mass

define p = g u c d s u~ c~ d~ s~
define j = g u c d s u~ c~ d~ s~
define l+ = e+ mu+
define l- = e- mu-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~
define p = p b b~
define j = j b b~
define ell+ = e+ mu+ ta+
define ell- = e- mu- ta-
define wdecay = j ell+ vl ell- vl~
define wpm = w+ w-

generate p p > t t~ wpm NP=1
#generate p p > t t~ wpm, wpm > wdecay wdecay @0
#add process p p > t t~ wpm j, wpm > wdecay wdecay @1
#add process p p > t t~ wpm j j, wpm > wdecay wdecay @3
# output ttW012j_5f -nojpeg

output processtmp -nojpeg
