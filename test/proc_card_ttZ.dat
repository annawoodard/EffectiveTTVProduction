set group_subprocesses Auto
set ignore_six_quark_processes False
set loop_optimized_output True
set complex_mass_scheme False
import model HEL_UFO-no_b_mass

define p = g u c d s u~ c~ d~ s~
define j = g u c d s u~ c~ d~ s~
define l+ = e+ mu+
define l- = e- mu-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~
define p = p b b~
define j = j b b~

generate p p > t t~ z NP=1

# generate p p > t t~ z, z > j j @0
# add process p p > t t~ z, z > vl vl~ @1
# add process p p > t t~ l+ l- @2
# add process p p > t t~ z j, z > j j @3
# add process p p > t t~ z j, z > vl vl~ @4
# add process p p > t t~ l+ l- j @5
output processtmp -nojpeg
